{
  {% for region in regions %}
  "{{region}}": {
    {% for env in envs %}
    "{{env}}":{
      "api_nginx": {
        "api_nginx":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "consul": {
        "consul":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "eps": {
        "eps":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "mongo": {
        "mongo":["service"],
        "mongo_backup":["mongo_backup"],
        "dbscript":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "nat": {
        "nat":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "pingfederate": {
        "pingfederate":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "postgres": {
        "postgres":["service"],
        "postgres_backup": ["postgres_backup"],
        "dbscript":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "rabbitmq": {
        "rabbitmq":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "signal-dispatcher":{
        "signal-dispatcher":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "signal-transmitter":{
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "tokenservice": {
        "tokenservice":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      },
      "vpn": {
        "vpn":["service"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      }
    },
    {% endfor %}
    "infra": {
      "docker_registry": {
        "docker_registry": [ "service" ]
      },
      "gerrit": {
        "gerrit": [ "service" ]
      },
      "nexus": {
        "nexus": [ "service" ]
      },
      "signal_recipient": {
        "signal_recipient": [ "service" ]
      },
      "jenkins_master": {
        "jenkins_master":["service"],
        "ec2_deployer":["ec2_deployer"],
        "elb_deployer":["elb_deployer"],
        "iam_deployer":["iam_deployer"],
        "route53_deployer":["route53_deployer"],
        "default":["ec2", "summon", "logstash", "logs_backup"]
      }
    },
    "corp": {
      "es": {
        "es": [ "service" ]
      },
      "icinga": {
        "icinga": [ "service", "ec2_readonly" ]
      },
      "kibana": {
        "kibana": [ "service" ],
        "elastalert": [ "service" ]
      },
      "logstash": {
        "logstash": [ "service" ]
      }
    }
  }{%- if not loop.last -%},{%- endif %}
  {% endfor %}
}
